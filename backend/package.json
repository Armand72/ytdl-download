{
  "name": "template-react",
  "version": "1.0.0",
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "src/server.ts",
  "scripts": {
    "test": "jest --watchAll --no-cache",
    "test:ci": "jest --no-cache",
    "server": "ts-node-dev --transpile-only --respawn ./src/server.ts",
    "client": "npm start --prefix client",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Armand72/template-react.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Armand72/template-react/issues"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "<rootDir>/client"
    ],
    "setupFilesAfterEnv": [
      "./src/routes/__test__/setup.ts"
    ],
    "globals": {
      "jest": {
        "isolatedModules": true
      }
    }
  },
  "homepage": "https://github.com/Armand72/template-react#readme",
  "dependencies": {
    "@types/adm-zip": "^0.4.34",
    "@types/archiver": "^5.1.0",
    "adm-zip": "^0.5.5",
    "archiver": "^5.3.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jest": "26.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.13",
    "react": "^17.0.1",
    "socket.io": "^3.0.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3",
    "uuid": "^8.3.2",
    "ytdl-core": "^4.7.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.19",
    "@types/mongoose": "^5.10.3",
    "@types/react": "^17.0.0",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4"
  }
}
